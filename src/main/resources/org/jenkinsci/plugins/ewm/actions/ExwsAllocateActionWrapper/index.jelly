<?jelly escape-by-default='true'?>
<st:compress xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:l="/lib/layout">
    <l:layout title="${it.displayName}" norefresh="true">
        <st:include it="${it.parent}" page="sidepanel.jelly"/>
        <l:main-panel>
            <j:forEach var="exwsAction" items="${it.allActions}">
                <st:include it="${exwsAction}" page="index.jelly"/>
            </j:forEach>
        </l:main-panel>
    </l:layout>
</st:compress>
